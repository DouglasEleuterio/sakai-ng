{
    "name": "crm",
    "version": "1.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --port 4201",
        "build": "ng build",
        "build:docker": "ng build -c prod",
        "build:docker:prod": "ng build -c prod --output-path docker/build --base-href /angular/ --deploy-url /angular/",
        "clean": "rm -r node_modules",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "format": "prettier --write src/**/*.{js,ts,html}",
        "coverage": "ng test --code-coverage --watch=false",
        "sonar:local": "sonar-scanner",
        "replace-version": "node replace-version.js"
    },
    "private": true,
    "dependencies": {
        "primeng": "17.13.0",
        "@angular-devkit/build-angular": "^17.3.1",
        "@angular/animations": "^17.3.1",
        "@angular/cdk": "^17.3.1",
        "@angular/cli": "^17.3.1",
        "@angular/common": "^17.3.1",
        "@angular/compiler": "^17.3.1",
        "@angular/compiler-cli": "^17.3.1",
        "@angular/core": "^17.3.1",
        "@angular/forms": "^17.3.1",
        "@angular/platform-browser": "^17.3.1",
        "@angular/platform-browser-dynamic": "^17.3.1",
        "@angular/platform-server": "^17.3.1",
        "@angular/router": "^17.3.1",
        "chart.js": "3.3.2",
        "primeflex": "^3.3.1",
        "primeicons": "^7.0.0",
        "quill": "1.3.7",
        "rxjs": "~7.8.1",
        "tslib": "^2.5.0",
        "zone.js": "~0.14.0",
        "@fullcalendar/angular": "^6.0.3",
        "@fullcalendar/core": "^6.0.3",
        "@fullcalendar/daygrid": "^6.0.3",
        "@fullcalendar/interaction": "^6.0.3",
        "@fullcalendar/timegrid": "^6.0.3",
        "prismjs": "^1.29.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^17.0.5",
        "@angular/cli": "^17.0.5",
        "@angular/compiler-cli": "^17.0.5",
        "@types/jasmine": "~4.3.1",
        "jasmine-core": "~4.6.0",
        "karma": "~6.4.2",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "replace-in-file": "^6.3.5",
        "typescript": "~5.2.2"
    }
